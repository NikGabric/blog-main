{
  "version": 1,
  "paths": {
    "/posts/{postId}": {
      "name": "/posts/{postId}",
      "lambdaFunction": "PostsHandler",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    }
  }
}